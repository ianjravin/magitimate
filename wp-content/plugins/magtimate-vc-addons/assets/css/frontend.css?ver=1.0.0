  display: block;
}
.wpcf7-form .wpcf7-form-control.wpcf7-submit {
  padding: 10px 20px;
  background-color: #54b9ff;
  transition: all 0.5s;
  color: #fff;
  font-size: 12px;
  text-transform: uppercase;
  font-family: 'Raleway', Arial, sans-serif;
  font-weight: 700;
  display: inline-block;
  border: none;
}
.wpcf7-form .wpcf7-form-control.wpcf7-submit:hover {
  background-color: #888;
  text-decoration: none;
}
.wpcf7-form .wpcf7-form-control-wrap {
  padding: 0;
  text-indent: 0;
  display: block;
  margin-bottom: 15px;
  position: relative;
}
.wpcf7-form .wpcf7-form-control-wrap:after {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 3px;
  height: 100%;
  background-color: #888;
  content: "";
}
.wpcf7-form .wpcf7-form-control-wrap.your-message:after {
  display: none;
}
.wpcf7-form .wpcf7-form-control-wrap .wpcf7-form-control {
  border: 1px solid #ddd;
  width: 100%;
  padding: 10px 15px;
}
.wpcf7-form .wpcf7-form-control-wrap .wpcf7-form-control:focus {
  outline: none;
}
.wpcf7-form .wpcf7-form-control-wrap .wpcf7-textarea {
  height: 175px;
  line-height: 1;
}
.wpcf7-form .wpcf7-form-control-wrap .wpcf7-not-valid {
  border: 1px solid red;
}
.wpcf7-form .wpcf7-response-output {
  text-align: center;
}
.post-item .entry-thumb {
  overflow: hidden;
}
.post-item .entry-thumb img {
  transition: 0.4s;
}
.post-item:hover .entry-thumb img {
  transform: scale(1.05);
}
.posts-slider-widget .post-item .overlay-post {
  z-index: 1;
}
.posts-slider-widget .post-item .entry-header {
  z-index: 2;
}
.posts-slider-widget .post-item:hover .entry-thumb img {
  transform: scale(1);
}
